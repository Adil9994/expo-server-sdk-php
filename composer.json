{
    "name": "ctwillie/expo-server-sdk-php",
    "description": "Server-side library for working with Expo using PHP",
    "keywords": [
        "ctwillie",
        "expo",
        "php",
        "expo-server-sdk-php"
    ],
    "homepage": "https://github.com/ctwillie/expo-server-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Twillie",
            "email": "ctwillie77@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "league/flysystem": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Twillie\\Expo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twillie\\Expo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
